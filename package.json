    "url": "https://gitlab.com/tactech/boilerplate-api-node/issues"
  },
  "homepage": "https://gitlab.com/tactech/boilerplate-api-node#readme",
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@sendgrid/mail": "^7.2.6",
    "aws-sdk": "^2.732.0",
    "axios": "^0.20.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.3.1",
